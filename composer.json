{
  "name": "rugalib/ruga-db-person",
  "description": "Common person entity",
  "license": "AGPL-3.0-only",
  "homepage": "https://github.com/rugalib",
  "type": "library",
  "require": {
    "php": "^7.4|^8.0|^8.1|^8.2",
    "psr/container": "^1.0",
    "rugalib/ruga-db": "^1.0",
    "rugalib/ruga-log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "laminas/laminas-config-aggregator": "^1",
    "laminas/laminas-servicemanager": "^3"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.rugalib.ch/"
    }
  ],
  "authors": [
    {
      "name": "Roland Rusch",
      "email": "roland.rusch@easy-smart.ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ruga\\Person\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ruga\\Person\\Test\\": "test/src"
    }
  },
  "extra": {
    "laminas": {
      "component": "Ruga\\Person",
      "config-provider": "Ruga\\Person\\ConfigProvider"
    }
  }
}